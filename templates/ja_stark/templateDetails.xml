	<languages folder="language">
		<language tag="en-GB">en-GB/en-GB.tpl_ja_stark.ini</language>
		<language tag="en-GB">en-GB/en-GB.tpl_ja_stark.sys.ini</language>
	</languages>
	<t4>
		<basetheme>base</basetheme>
		<layouts>
			<fields name="t4layout">
				<field name="styles_palettes" class="color_pattern_group" class_field="col-12" type="palettes" icon="swatchbook" group="palettes" 
		    			label="T4_COLOR_PATTERN_GROUP" description="T4_COLOR_PATTERN_DESC" >
					<color name="background_color" title="T4_LAYOUT_PALETTES_BACKGROUND_COLOR"  value="" class="bg_color" />
					<color name="text_color" title="T4_LAYOUT_PALETTES_TEXT_COLOR"  value="" class="text_color" />
					<color name="link_color" title="T4_LAYOUT_PALETTES_LINK_COLOR"  value="" class="link_color" />
					<color name="link_hover_color" title="T4_LAYOUT_PALETTES_LINK_HOVER_COLOR"  value="" class="link_hover_color" />
					<color name="heading_color" title="TPL_T4_HEADING_COLOR"  value="" class="heading_color" />
				</field>
			</fields>
		</layouts>
	</t4>
	
	<updateservers>
    	<server type="extension">http://update.joomlart.com/service/tracking/j31/tpl_ja_stark.xml</server>
	</updateservers>
</extension>
